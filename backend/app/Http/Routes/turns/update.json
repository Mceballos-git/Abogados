{
  "identifier": "update",
  "requestType": "PUT",
  "endpoint": "/{id}",
  "controller": "App\\Http\\Controllers\\TurnController",
  "action": "update",
  "authorizationMiddleware": "false",
  "authenticationMiddleware": "true",
  "validationMiddleware": "true",
  "validationRules": {
    "client_id": "required|integer",
    "given_user_id": "required|integer",
    "attention_user_id": "required|integer",
    "register_date": "required|date_format:Y-m-d",
    "turn_date": "required|date_format:Y-m-d",
    "turn_time_start": "required|date_format:H:i:s",
    "turn_time_end": "required|date_format:H:i:s",
    "phone_number_ref": "required|string",
    "priority": "sometimes",
    "procedure_category_id":"sometimes",
    "comments": "sometimes",
    "title": "sometimes",
    "active": "required|integer"
  }
}
